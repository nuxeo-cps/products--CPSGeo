<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 3.2 Final//EN">
<html>
<head>
<title>CPSGeo Map Widget Demo</title>
<link rel="stylesheet" href="/mapbuilder/lib/skin/default/mapStyle.css" type="text/css">
<script>
    // URL of Mapbuilder configuration file.
    var mbConfigUrl='geo_config_live.xml';    
</script>
<script type="text/javascript" src="/mapbuilder/lib/Mapbuilder.js"></script>
</head>
<body onload="mbDoLoad()">
<table>
    <tr> 
        <td>&nbsp;</td>
        <td><h2>CPSGeo Map Browser</h2></td>
    </tr>
    <tr>
        <!-- left hand column -->
        <td valign="top" style="padding:10px">
            <div><div id="locatorMap"/></div>
            <div><div id="legend"/></div>
        </td>
        <!-- right hand column -->
        <td> <table width="100%">
                <tr> 
                    <td align="left"> <h2 id="mapTitle"></h2></td>
                </tr>
                <tr> 
                    <td align="center"> <table>
                            <tr> 
                                <td colspan="3"> <div id="mainMapPane"/>
                                    <div id="loading"> 
                                        <p>Loading Program<br/>
                                            <img src="/mapbuilder/lib/skin/default/images/Loading.gif"/> 
                                        </p>
                                    </div></td>
                            </tr>
                            <tr> 
                                <td align="left"> <div id="mainButtonBar"/></td>
                                <td align="center" id="mapScaleText" />&nbsp;
                                <td align="right" id="cursorTrack" />&nbsp;</tr>
                        </table></td>
                </tr>
            </table>
    </tr>
</table>

<table>
    <tr>
        <td align="left">
            Simulated Document Query Results Below
            <hr/>
            <div id="eventsListing"></div>
        </td>
        <td align="left">
        Mouse over rows in the query results to see documents highlighted in
        the map frame.
        </td>
    </tr>
</table>

</body>
</html>
